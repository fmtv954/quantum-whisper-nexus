3. Lead Management Interface

3.1 Leads Table Component

```typescript
// Leads table specification
interface LeadsTable {
  columns: TableColumn[];
  data: Lead[];
  pagination: PaginationConfig;
  sorting: SortingConfig;
  filtering: FilterConfig;
  selection: SelectionConfig;
  actions: TableAction[];
}

interface TableColumn {
  key: string;
  label: string;
  sortable: boolean;
  filterable: boolean;
  width?: string;
  render?: (value: any, row: Lead) => ReactNode;
}

const leadColumns: TableColumn[] = [
  {
    key: 'select',
    label: '',
    sortable: false,
    filterable: false,
    width: '40px',
    render: (_, row) => <SelectionCheckbox lead={row} />
  },
  {
    key: 'status',
    label: 'Status',
    sortable: true,
    filterable: true,
    width: '100px',
    render: (value, row) => <StatusBadge status={value} lead={row} />
  },
  {
    key: 'full_name',
    label: 'Contact',
    sortable: true,
    filterable: true,
    width: '200px',
    render: (value, row) => <ContactCell lead={row} />
  },
  {
    key: 'qualification_score',
    label: 'Score',
    sortable: true,
    filterable: true,
    width: '100px',
    render: (value) => <QualificationScore score={value} />
  },
  {
    key: 'urgency_level',
    label: 'Urgency',
    sortable: true,
    filterable: true,
    width: '100px',
    render: (value) => <UrgencyIndicator level={value} />
  },
  {
    key: 'created_at',
    label: 'Received',
    sortable: true,
