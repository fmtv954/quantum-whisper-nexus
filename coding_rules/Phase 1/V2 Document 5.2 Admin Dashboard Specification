5. Cost Monitoring & Optimization

5.1 Real-Time Cost Dashboard

```typescript
// Cost monitoring components
interface CostDashboard {
  overview: CostOverview;
  breakdown: CostBreakdown;
  trends: CostTrends;
  alerts: CostAlerts;
  recommendations: OptimizationRecommendations;
}

interface CostOverview {
  totalSpend: number;
  spendToday: number;
  projectedMonthly: number;
  budgetUtilization: number;
  costPerLead: number;
  costPerMinute: number;
}

interface CostBreakdown {
  byService: ServiceCost[];
  byCampaign: CampaignCost[];
  byTime: TimeCost[];
}

interface ServiceCost {
  service: 'deepgram_stt' | 'deepgram_tts' | 'openai' | 'livekit' | 'gemini' | 'tavily';
  cost: number;
  percentage: number;
  usage: number;
  efficiency: number;
}

// Cost alert system
interface CostAlert {
  id: string;
  type: 'budget' | 'anomaly' | 'efficiency';
  severity: 'low' | 'medium' | 'high' | 'critical';
  title: string;
  message: string;
  triggeredAt: Date;
  resolved: boolean;
  action?: AlertAction;
}

const costAlerts: CostAlert[] = [
  {
    id: 'budget_80_percent',
    type: 'budget',
    severity: 'medium',
    title: 'Budget Utilization at 80%',
    message: 'Monthly budget is 80% utilized. Consider increasing budget or optimizing usage.',
    triggeredAt: new Date(),
    resolved: false,
    action: {
      label: 'Adjust Budget',
      handler: () => openBudgetSettings()
    }
  },
  {
    id: 'high_tts_cost',
    type: 'efficiency',
    severity: 'high',
    title: 'High TTS Cost Detected',
    message: 'TTS costs are 40% above average. Consider optimizing response lengths.',
    triggeredAt: new Date(),
    resolved: false,
    action: {
      label: 'View Optimization',
      handler: () => openTTSSettings()
    }
  }
];
```

